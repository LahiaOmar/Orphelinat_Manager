<div id="panel">
    <div class="title">
        <h1>تسجيل المداخيل</h1>
    </div>
    <form >
        <input type="hidden" name="mod" value="income" class="text">
        <div class="form_element">
            <div class="input"><input type="text" name="source" class="text" required pattern=".{1,}"></div>
            <div class="label"><label for="">مصدر المداخيل</label></div>
        </div>
        <div class="form_element">
            <div class="input"><input type="text" name="accountantCode" class="text" required pattern=".{1,}"></div>
            <div class="label"><label for="">الرمز المحاسبي</label></div>
        </div>
        <div class="form_element">
            <div class="input"><input type="text" name="documentNumber" class="text" required></div>
            <div class="label"><label for="">رقم وثيقة التحويل</label></div>
        </div>
        <div class="form_element">
            <div class="input"><input type="date" name="transferDate" class="date" required></div>
            <div class="label"><label for="">تاريخ التحويل</label></div>
        </div>
        <div class="form_element">
            <div class="input"><input type="text" name="value"  class="text" required></div>
            <div class="label"><label for="">مبلغ التحويل بالدرهم</label></div>
        </div>
        <div class="form_element">
            <div class="input"><input type="text" name="note" class="text"></div>
            <div class="label"><label for="">ملاحظات</label></div>
        </div>

        <div class="form_element">
            <div class="input"><input type="date" name="day" class="date" required=""> </div> <div class="label"><label for="">يوم</label></div>
        </div>
        <div class="submitLoding">
            <img class="loding" src="../images/reload.png" alt="">
            <button type="submit" id="submit"class="submit">تسجيل</button>
        </div>
    </form>
</div>